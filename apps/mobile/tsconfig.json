      "@connecthealth/identity/application": ["libs/identity/application/src/index.ts"],
      "@connecthealth/identity/infrastructure": ["libs/identity/infrastructure/src/index.ts"],
      "@connecthealth/identity/ui": ["libs/identity/ui/src/index.ts"],
      "@connecthealth/client/domain": ["libs/client/domain/src/index.ts"],
